{
  "name": "exploramer",
  "version": "1.0.0",
  "private": true,
  "main": "main.js",
  "build": {
    "appId": "com.benoitmartel.exploramer",
    "extraResources": [
      {
        "from": "./static/extraResources/",
        "to": "extraResources",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "target": {
        "target": "NSIS"
      },
      "icon": "static/icon.png"
    },
    "directories": {
      "output": "app"
    }
  },
  "scripts": {
    "dev": "nuxt",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:prettier",
    "lintfix": "prettier --write --list-different .",
    "electron": "cross-env NODE_ENV=dev electron .",
    "e:build": "cross-env NODE_ENV=production nuxt build && electron-builder --win"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "aframe-transparent-video-shader": "^1.0.3",
    "body-parser": "^1.20.1",
    "core-js": "^3.19.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "nuxt": "^2.15.8",
    "sqlite3": "^5.1.4",
    "vue": "^2.6.14",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "vuex-persist": "^3.1.3",
    "vuex-persistedstate": "^4.1.0",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "cross-env": "^5.1.4",
    "electron": "^13.1.2",
    "electron-builder": "^22.10.5",
    "electron-devtools-installer": "^2.2.3",
    "prettier": "^2.5.1"
  }
}
